{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^/.*$": {
      "type": "object",
      "properties": {
        "location": {
          "type": "string"
        },
        "expiration": {
          "type": "integer"
        },
        "user": {
          "type": "string"
        },
        "created": {
          "type": "integer"
        }
      },
      "required": ["location"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
