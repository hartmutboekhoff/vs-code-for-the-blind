{
    "name": "High Contrast Large Font Light",
    "type": "light",
    "colors": {

        "list.hoverBackground": "#FFA",
        "list.hoverForeground": "#220",
        "list.highlightBackground": "#f00",
        "list.highlightForeground": "#fcc",
        
        "list.activeSelectionBackground": "#a95",
        "list.inactiveSelectionBackground": "#965",
        
        "sideBarStickyScroll.background": "#875",
        "sideBarStickyScroll.border": "#ff0",
        
        
        "editorGroupHeader.tabsBackground": "#848",
        "editorGroupHeader.noTabsBackground": "#848",
        "tab.activeBackground": "#b0b",
        "tab.activeForeground": "#fff",
        "tab.inactiveBackground": "#848",
        "tab.inactiveForeground": "#aaa",

        "_tab.unfocusedActiveBackground": "#000000",
        "_tab.border": "#000000",
        "_tab.activeBorder": "#000000",
        "_tab.selectedBorderTop": "#000000",
        "_tab.selectedBackground": "#FF8800",
        "_tab.selectedForeground": "#000000",


    "breadcrumb.foreground": "#000",
    "breadcrumb.background": "#ff0",
    "breadcrumb.focusForeground": "#00f",
    "breadcrumb.activeSelectionForeground": "#0f0",
    "_breadcrumbPicker.background": "#dd7",
    "breadcrumbPicker.focusForeground": "#000",


        
        "editor.background": "#FFF",
        "editor.foreground": "#000000",
        "editorLineNumber.foreground": "#C80",
        "editorLineNumber.activeForeground": "#840",
        
        "editor.lineHighlightBackground": "#ff4",
        "editor.selectionBackground": "#8DB6DF",
        "editor.selectionHighlightBackground": "#FF0000",
        "editorCursor.foreground": "#0000FF",
        "editorWhitespace.foreground": "#BFBFBF",
        "editor.findMatchBackground": "#FFA500",
        "editor.findMatchHighlightBackground": "#FFE4B5",
        "editor.wordHighlightBackground": "#aac",
        
        "editorStickyScrollHover.background": "#daf",

        "_editor.foldBackground": "#444",
        "_editor.foldPlaceholderForeground": "#ccc",

        "editorGutter.background": "#fcf",
        "editorGutter.foldingControlForeground": "#f0f",
        
        "peekViewEditor.background": "#444",
        
        "notifications.background": "#222",
        
        "sideBar.background": "#255",
        "sideBar.foreground": "#FFF",
        "-sideBarTitle.foreground": "#000000",
        "sideBarSectionHeader.background": "#919",
        "sideBarSectionHeader.foreground": "#FFF",
        
        "-activityBar.background": "#FFFFFF",
        "-activityBar.foreground": "#000000",
        "-activityBar.border": "#BFBFBF",
        "-activityBarBadge.background": "#0000FF",
        "-activityBarBadge.foreground": "#FFFFFF",
        
        "-statusBar.background": "#0000FF",
        "-statusBar.foreground": "#FFFFFF",
        "-statusBar.noFolderBackground": "#8800FF",
        "-statusBar.debuggingBackground": "#FF0000",
        
        "-titleBar.activeBackground": "#F0F0F0",
        "-titleBar.activeForeground": "#000000",
        "-titleBar.inactiveBackground": "#E0E0E0",
        "-titleBar.inactiveForeground": "#666666",
        
        "-list.activeSelectionBackground": "#0000FF",
        "-list.activeSelectionForeground": "#FFFFFF",
        "-list.hoverBackground": "#E6F3FF",
        "-list.highlightForeground": "#0000FF",
        
        "-input.background": "#FFFFFF",
        "-input.foreground": "#000000",
        "-input.border": "#0000FF",
        "-input.placeholderForeground": "#666666",
        
        "-button.background": "#0000FF",
        "-button.foreground": "#FFFFFF",
        "-button.hoverBackground": "#0066FF"
    },
    "tokenColors": [
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#008000",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#A31515"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#0000FF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#795E26",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#001080"
            }
        },
        {
            "scope": ["constant.numeric", "constant.language"],
            "settings": {
                "foreground": "#098658"
            }
        },
        {
            "scope": ["entity.name.type", "support.type"],
            "settings": {
                "foreground": "#267F99"
            }
        },
        {
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#800000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "scope": ["markup.heading"],
            "settings": {
                "foreground": "#800000",
                "fontStyle": "bold"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": "#001080",
        "type": "#267F99",
        "function": "#795E26",
        "variable": "#001080",
        "number": "#098658",
        "string": "#A31515",
        "keyword": "#0000FF",
        "operator": "#000000",
        "class": "#267F99"
    }
} 